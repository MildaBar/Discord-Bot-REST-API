{
  "name": "discord-bot-rest-api",
  "description": "Discord bot with a REST API",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.52.0",
    "kysely-codegen": "^0.11.0",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "scripts": {
    "test": "vitest",
    "dev": "tsnd --respawn src",
    "coverage": "vitest run --coverage",
    "gen:types": "kysely-codegen --camel-case --dialect sqlite --out-file src/database/types.ts",
    "migrate:latest": "ts-node src/database/migrate/migrate.ts",
    "seed:run": "ts-node src/database/seeders/*.ts"
  },

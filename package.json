  "dependencies": {
    "@giphy/js-fetch-api": "^5.2.0",
    "@types/better-sqlite3": "^7.6.6",
    "axios": "^1.6.0",
    "better-sqlite3": "^9.0.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "kysely": "^0.26.3",
    "zod": "^3.22.4"
  }
}
